                android:textColor="@color/skyblue"
                android:textSize="17sp"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/dream4"
                android:paddingBottom="12dp"
